{
  "manifest_version": 2,
  "name": "SharePoint Column \\ View Formatting extender",
  "description": "Extends SharePoint Column \\ View Formatting with convinient editing experience",
  "version": "1.0",
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "dist/content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "hot-reload.js",
      "dist/background.js"
    ]
  },
  "permissions": [
  ],
  "web_accessible_resources": [
    "dist/*"
  ]
}